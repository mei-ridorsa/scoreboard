{
    "name": "mei-ridorsa/scoreboard",
    "type": "project",
    "license": "MIT",
    "description": "PHP hexagonal architecture and DDD practice",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpro/grumphp-shim": "^1.13",
        "phpunit/phpunit": "^9.5",
        "brianium/paratest": "^6.6",
        "friendsofphp/php-cs-fixer": "^3.11",
        "nikic/php-parser": "^4.15",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpspec/phpspec": "^7.2",
        "squizlabs/php_codesniffer": "*",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "^6.1"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "symfony/dotenv": "^6.2",
        "symfony/console": "^6.2",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/runtime": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "ScoreBoard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoreBoard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
